                                <apex:selectOption itemValue="Both" itemLabel="Both" />
                                <apex:selectOption itemValue="BUYING" itemLabel="BUYING" />
                                <apex:selectOption itemValue="SELLING" itemLabel="SELLING" />
                            </apex:selectList>
                        </apex:pageBlockSectionItem>
                    
                </apex:pageBlockSection>
            </apex:outputPanel>
        </apex:pageBlock>
        
