                                <apex:param value="{!item.BuyInTaxAmountUSD__c}" />
                            </apex:outputText>
                        </td>
                        <td style="text-align: right; padding-right:5px" class="{!IF(OR(objSearch.amountType=='SELLING',objSearch.currencyType=='VND' ), 'hideColumn', '')}">
