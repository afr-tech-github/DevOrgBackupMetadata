        <script type="text/javascript">
            var $j = jQuery.noConflict();
            var totalBuyInTaxAmountJPY = 0;
            var totalSiharaigaku = 0;
            var totalBuyDifferentiation = 0;
            var totalBuyInTaxAmountUSD = 0;
            var totalAMOUNT_PAID_BUYING_USD = 0;
            var totalUNPAID_BALANCE_OF_BUYING_USD = 0;
            var totalSellInTaxAmountJPY = 0;
            var totalNyukingaku = 0;
            var totalSellDifferentiation = 0;
            var totalSellInTaxAmount_USD = 0;
            var totalAMOUNT_PAID_SELLING_USD = 0;
            var totalUNPAID_BALANCE_OF_SELLING_USD = 0;
            function doChangeTab(element) {
                af_ChangeTab(element);
            }
            function checkAllRecord(){
                
                if(j$('#billItemAll').is(":checked")){
                    j$('.billItem').prop( "checked", true);
                    doSumAll();
                }else{
                    j$('.billItem').prop( "checked", false);
                    refreshTotalValues();
                }
            }
            function toggleCheckbox(event, item){
                var recordId = event.target.id, checked = event.target.checked;
                if(recordId) {
                    doSum(recordId, checked);
                    doShowSum();
                }
            }
            function doSum(recordId, checked){
                var weight = checked ? 1 : -1;
                var currentRec = listBillFMSString.find(x => x.Id == recordId);
                totalBuyInTaxAmountJPY += weight * (currentRec.clofor_com_cfs__BuyInTaxAmountJPY__c || 0);
                totalSiharaigaku += weight * (currentRec.clofor_com_cfs__Siharaigaku__c || 0);
                totalBuyDifferentiation += weight * (currentRec.clofor_com_cfs__BuyDifferentiation__c || 0);
                totalBuyInTaxAmountUSD += weight * (currentRec.clofor_com_cfs__BuyInTaxAmountUSD__c || 0);
                totalAMOUNT_PAID_BUYING_USD += weight * (currentRec.clofor_com_cfs__AMOUNT_PAID_BUYING_USD__c || 0);
                totalUNPAID_BALANCE_OF_BUYING_USD += weight * (currentRec.clofor_com_cfs__UNPAID_BALANCE_OF_BUYING_USD__c || 0);
                totalSellInTaxAmountJPY += weight * (currentRec.clofor_com_cfs__SellInTaxAmountJPY__c || 0);
                totalNyukingaku += weight * (currentRec.clofor_com_cfs__Nyukingaku__c || 0);
                totalSellDifferentiation += weight * (currentRec.clofor_com_cfs__SellDifferentiation__c || 0);
                totalSellInTaxAmount_USD += weight * (currentRec.clofor_com_cfs__SellInTaxAmount_USD__c || 0);
                totalAMOUNT_PAID_SELLING_USD += weight * (currentRec.clofor_com_cfs__AMOUNT_PAID_SELLING_USD__c || 0);
                totalUNPAID_BALANCE_OF_SELLING_USD += weight * (currentRec.clofor_com_cfs__UNPAID_BALANCE_OF_SELLING_USD__c || 0);
            }
            function doSumAll(){
                console.log('doSumAll listBillFMSString: ', listBillFMSString.length);
                listBillFMSString.forEach(x => {
                    doSum(x.Id, true);
                });
                doShowSum();
            }
            function doShowSum(){
                var tr = $j('.clsRowTotal').find('th');
    		    tr.eq(0).html(formatNumber(totalBuyInTaxAmountJPY));
    		    tr.eq(1).html(formatNumber(totalSiharaigaku));
    		    tr.eq(2).html(formatNumber(totalBuyDifferentiation));
    		    tr.eq(3).html(formatNumber(totalBuyInTaxAmountUSD));
    		    tr.eq(4).html(formatNumber(totalAMOUNT_PAID_BUYING_USD));
    		    tr.eq(5).html(formatNumber(totalUNPAID_BALANCE_OF_BUYING_USD));
    		    tr.eq(6).html(formatNumber(totalSellInTaxAmountJPY));
    		    tr.eq(7).html(formatNumber(totalNyukingaku));
    		    tr.eq(8).html(formatNumber(totalSellDifferentiation));
    		    tr.eq(9).html(formatNumber(totalSellInTaxAmount_USD));
    		    tr.eq(10).html(formatNumber(totalAMOUNT_PAID_SELLING_USD));
    		    tr.eq(11).html(formatNumber(totalUNPAID_BALANCE_OF_SELLING_USD));
            }
            function formatNumber(num){
                if(!num || typeof num !== 'number'){
                    return 0;
                } else {
                    var numString = num.toString();
                    var decimalPlaces = (numString.includes(".") && numString.substr(numString.indexOf('.') + 1).length) || 0;
                    decimalPlaces = decimalPlaces > 3 ? 3 : decimalPlaces;
                    return Number(num).toFixed(decimalPlaces).toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                }
            }
            function refreshTotalValues(){
                totalBuyInTaxAmountJPY = 0;
                totalSiharaigaku = 0;
                totalBuyDifferentiation = 0;
                totalBuyInTaxAmountUSD = 0;
                totalAMOUNT_PAID_BUYING_USD = 0;
                totalUNPAID_BALANCE_OF_BUYING_USD = 0;
                totalSellInTaxAmountJPY = 0;
                totalNyukingaku = 0;
                totalSellDifferentiation = 0;
                totalSellInTaxAmount_USD = 0;
                totalAMOUNT_PAID_SELLING_USD = 0;
                totalUNPAID_BALANCE_OF_SELLING_USD = 0;
                doShowSum();
            }
            function insertRecord(){
                var billcheckeds = j$('.billItem:checkbox:checked');
                var billIds = [];
                for(var i =0; i<billcheckeds.length; i++){
                   var idbill = billcheckeds[i].id;
                   billIds.push(idbill);
                };
                var strbillIds = billIds.join(',');
                if(!strbillIds){
                    alert('Please select at least 1 bill to combine.');
                }else{
                    /*if(billIds.length > 12){
                        alert('Maximum 12 records can be handled in once time.');
                        
                    }else{
                        af_insertbillPlan(strbillIds);
                    }*/
                    af_insertbillFMSBank(strbillIds);
                    
                }
            }
        </script>
