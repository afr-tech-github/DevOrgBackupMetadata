                                    <apex:outputLabel style="line-height: 23.5px;" value="/" rendered="{!customer.Fax!=null && customer.Phone!=null}" />
                                    <apex:outputLabel style="line-height: 23.5px;" value="{!translations['fax']}" rendered="{!customer.Fax!=null}" />
                                    <apex:outputLabel style="line-height: 23.5px;" value=": " rendered="{!customer.Phone!=null || customer.Fax!=null}" />
                                    <apex:outputLabel style="line-height: 23.5px;" value="{!customer.Phone}" rendered="{!customer.Phone!=null}" />
                                    <apex:outputLabel style="line-height: 23.5px;" value="/" rendered="{!customer.Fax!=null && customer.Phone!=null}" />
                                    <apex:outputLabel style="line-height: 23.5px;" value="{!customer.Fax}" rendered="{!customer.Fax!=null}" />
                                    <apex:outputLabel style="line-height: 23.5px;" value=" 　　" rendered="{!customer.TAX_CODE__c!=null && (customer.Phone!=null || customer.Fax!=null)}"
                                    />
                                    <apex:outputLabel style="line-height: 23.5px;" value="{!translations['tin']}: {!customer.TAX_CODE__c}" rendered="{!customer.TAX_CODE__c!=null}"
