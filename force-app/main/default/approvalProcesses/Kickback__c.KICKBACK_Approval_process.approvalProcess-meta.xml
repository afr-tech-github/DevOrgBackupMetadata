    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
        <field>clofor_com_cfs__ShipmentOrder__c</field>
        <field>clofor_com_cfs__PricingID__c</field>
        <field>clofor_com_cfs__ChargeUnit__c</field>
        <field>clofor_com_cfs__KickbackName__c</field>
        <field>clofor_com_cfs__KickbackTo__c</field>
        <field>clofor_com_cfs__UnitPrice__c</field>
        <field>clofor_com_cfs__TotalKickbackAmount__c</field>
        <field>clofor_com_cfs__UnitPriceUSD__c</field>
        <field>clofor_com_cfs__TotalKickbackAmountUSD__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>afr-dev1@afr-solutions.com</name>
                <type>user</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <booleanFilter>(1 OR 2) AND (3 OR 4)</booleanFilter>
            <criteriaItems>
                <field>clofor_com_cfs__Kickback__c.clofor_com_cfs__TotalKickbackAmount__c</field>
                <operation>greaterThan</operation>
            </criteriaItems>
            <criteriaItems>
                <field>clofor_com_cfs__Kickback__c.clofor_com_cfs__TotalKickbackAmountUSD__c</field>
                <operation>greaterThan</operation>
            </criteriaItems>
            <criteriaItems>
                <field>clofor_com_cfs__Kickback__c.clofor_com_cfs__UnpaidBalance__c</field>
                <operation>greaterOrEqual</operation>
            </criteriaItems>
            <criteriaItems>
                <field>clofor_com_cfs__Kickback__c.clofor_com_cfs__UnpaidBalanceUSD__c</field>
                <operation>greaterOrEqual</operation>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>ApproveRecord</ifCriteriaNotMet>
        <label>KICKBACK_approval_step</label>
        <name>KICKBACK_approval_step</name>
    </approvalStep>
    <description>Use when a Kickback is submitted</description>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <entryCriteria>
        <booleanFilter>(1 OR 2) AND (3 OR 4)</booleanFilter>
        <criteriaItems>
            <field>clofor_com_cfs__Kickback__c.clofor_com_cfs__TotalKickbackAmount__c</field>
            <operation>greaterThan</operation>
            <value>0</value>
        </criteriaItems>
        <criteriaItems>
            <field>clofor_com_cfs__Kickback__c.clofor_com_cfs__TotalKickbackAmountUSD__c</field>
            <operation>greaterThan</operation>
            <value>0</value>
        </criteriaItems>
        <criteriaItems>
            <field>clofor_com_cfs__Kickback__c.clofor_com_cfs__UnpaidBalance__c</field>
            <operation>greaterOrEqual</operation>
            <value>0</value>
        </criteriaItems>
        <criteriaItems>
            <field>clofor_com_cfs__Kickback__c.clofor_com_cfs__UnpaidBalanceUSD__c</field>
            <operation>greaterOrEqual</operation>
            <value>0</value>
        </criteriaItems>
    </entryCriteria>
