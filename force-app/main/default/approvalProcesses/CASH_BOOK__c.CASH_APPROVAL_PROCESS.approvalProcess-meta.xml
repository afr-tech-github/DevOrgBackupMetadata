    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
        <field>clofor_com_cfs__CASH_STATUS__c</field>
        <field>clofor_com_cfs__DIFFERENCE_AMOUNT_ACTUAL_PAID__c</field>
        <field>clofor_com_cfs__DIFFERENCE_LEFT__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <approvalActions>
            <action>
                <name>CASH_APROVAL_EMAIL_ALERT</name>
                <type>Alert</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>Account_Team</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>STEP 1CASH APPROVAL PROCESS</label>
        <name>STEP_1CASH_APPROVAL_PROCESS</name>
    </approvalStep>
    <emailTemplate>Approval/CASH_BOOK_APPROVAL_PROCESS_REQUEST</emailTemplate>
    <enableMobileDeviceAccess>true</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>clofor_com_cfs__CASH_BOOK__c.Owner</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>clofor_com_cfs__CASH_BOOK__c.clofor_com_cfs__CASH_STATUS__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Approval_Status_Cash_Approved</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>true</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Approval_Status_Cash_Rejected</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>CASH_BOOK_APPROVAL_REJECTED</name>
            <type>Alert</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Approval_Status_Cash_Initial</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>CASH APPROVAL PROCESS</label>
    <processOrder>1</processOrder>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
