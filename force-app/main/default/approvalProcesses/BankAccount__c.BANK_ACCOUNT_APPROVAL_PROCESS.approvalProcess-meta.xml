<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>true</allowRecall>
    <allowedSubmitters>
        <type>creator</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Accounting_Manager</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Accounting_Staff</submitter>
        <type>role</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <submitter>Top_Executive</submitter>
        <type>role</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
        <field>clofor_com_cfs__BankMaster__c</field>
        <field>clofor_com_cfs__BankOfficialName__c</field>
        <field>clofor_com_cfs__BankBranchName__c</field>
        <field>clofor_com_cfs__BankAddress__c</field>
        <field>clofor_com_cfs__BankPhoneNumber__c</field>
        <field>clofor_com_cfs__SwiftCode__c</field>
        <field>clofor_com_cfs__BankAccountNumber__c</field>
        <field>clofor_com_cfs__BankAccountCurrency__c</field>
        <field>clofor_com_cfs__FullNameOfAccountHolder__c</field>
        <field>clofor_com_cfs__InternetBankingID__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <type>adhoc</type>
            </approver>
        </assignedApprover>
        <label>BANK ACCOUNT APPROVAL STEP1</label>
        <name>BANK_ACCOUNT_APPROVAL_STEP1</name>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <assignedApprover>
            <approver>
                <type>adhoc</type>
            </approver>
        </assignedApprover>
        <label>BANK ACCOUNT APPROVAL STEP2</label>
        <name>BANK_ACCOUNT_APPROVAL_STEP2</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
    </approvalStep>
    <emailTemplate>Approval/BankAccountApprovalRequest</emailTemplate>
    <enableMobileDeviceAccess>false</enableMobileDeviceAccess>
    <finalApprovalRecordLock>true</finalApprovalRecordLock>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <label>BANK ACCOUNT APPROVAL PROCESS</label>
    <processOrder>1</processOrder>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
