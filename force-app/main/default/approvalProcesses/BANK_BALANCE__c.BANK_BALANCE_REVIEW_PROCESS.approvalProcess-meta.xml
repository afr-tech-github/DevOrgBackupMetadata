    <approvalPageFields>
        <field>Name</field>
        <field>Owner</field>
        <field>clofor_com_cfs__BALANCE_AT_CLOSING__c</field>
        <field>clofor_com_cfs__CLOSING_DATE__c</field>
        <field>clofor_com_cfs__AMOUNT_RECEIVED_IN_H_BL__c</field>
        <field>clofor_com_cfs__AMOUNT_RECEIVED_NOT_IN_H_BL__c</field>
        <field>clofor_com_cfs__AMOUNT_PAID_IN_H_BL__c</field>
        <field>clofor_com_cfs__AMOUNT_PAID_NOT_IN_H_BL__c</field>
        <field>clofor_com_cfs__LINK_TO_RECEIPTANCE_REPORT__c</field>
        <field>clofor_com_cfs__DESCRIPTION__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>false</allowDelegate>
        <assignedApprover>
            <approver>
                <name>Account_Team</name>
                <type>queue</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>clofor_com_cfs__BANK_BALANCE__c.clofor_com_cfs__BALANCE_AT_CLOSING__c</field>
                <operation>notEqual</operation>
            </criteriaItems>
        </entryCriteria>
        <ifCriteriaNotMet>ApproveRecord</ifCriteriaNotMet>
        <label>STEP1-BANK BANLACE CHECK</label>
        <name>STEP1_BANK_BANLACE_CHECK</name>
    </approvalStep>
    <emailTemplate>Approval/BANK_BALANCE_APPROVAL_PROCESS_REQUEST</emailTemplate>
    <enableMobileDeviceAccess>true</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>clofor_com_cfs__BANK_BALANCE__c.clofor_com_cfs__CLOSING_DATE__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>clofor_com_cfs__BANK_BALANCE__c.clofor_com_cfs__BALANCE_AT_CLOSING__c</field>
            <operation>greaterThan</operation>
            <value>0</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>Approval_Status_BankBal_Approved</name>
            <type>FieldUpdate</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>true</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>Approval_Status_BankBal_Rejected</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>BANK_BALANCE_APPROVAL_REJECTED</name>
            <type>Alert</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>Approval_Status_BankBal_Initial</name>
            <type>FieldUpdate</type>
        </action>
    </initialSubmissionActions>
    <label>BANK BALANCE REVIEW PROCESS</label>
    <processOrder>1</processOrder>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
