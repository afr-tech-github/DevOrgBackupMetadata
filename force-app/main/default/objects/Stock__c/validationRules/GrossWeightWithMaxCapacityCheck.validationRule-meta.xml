<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>GrossWeightWithMaxCapacityCheck</fullName>
    <active>false</active>
    <errorConditionFormula>AND(RecordType.DeveloperName == &apos;StockIn&apos;,
 GrossWeight__c &gt;  MaxCapacityKG__c)</errorConditionFormula>
    <errorDisplayField>GrossWeight__c</errorDisplayField>
    <errorMessage>The Gross Weight of stock in can not greater than Max Capacity KG.</errorMessage>
</ValidationRule>
