<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DUE_DATE_later_than_RECEIVED_DAT</fullName>
    <active>true</active>
    <errorConditionFormula>InvoiceReceivedDate__c  &gt;  PaymentDueDate__c</errorConditionFormula>
    <errorDisplayField>PaymentDueDate__c</errorDisplayField>
    <errorMessage>PAYMENT DUE DATE must be later than PURCHASING INVOICE RECEIVED DATE.</errorMessage>
</ValidationRule>
