<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>defaultpreviousyearannualleave__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>IF(
YEAR(today())-YEAR( ContractStartDate__c )=1,
13-MONTH( ContractStartDate__c),0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Meaning: The total number of last year&apos;s annual leave by default. This number is calculated from start date of Labor Contract.</inlineHelpText>
    <label>Default previous year&apos;s annual leave</label>
    <precision>18</precision>
    <required>false</required>
    <scale>1</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
